A :: blob {
    a: int,
    b: float,
}

B :: blob {
    self: B,
}

start :: fn {
    // Basic types
    as_str(-1) <=> "-1"
    as_str(2.0) <=> "2.0"
    as_str(2.00000000000182) <=> "2.00000000000182"
    as_str("3.0") <=> "3.0"
    as_str(true) <=> "true"
    as_str(false) <=> "false"

    // List
    a := []
    a -> push' a
    a -> push' 1
    as_str(a) <=> "[[...], 1]"

    // Tuple
    as_str(()) <=> "()"
    as_str((1,)) <=> "(1,)"
    as_str((1, 2)) <=> "(1, 2)"

    // Set
    as_str({}) <=> "{}"
    as_str({1}) <=> "{1}"
    s :: as_str({1, 2})
    (s == "{1, 2}" || s == "{2, 1}") <=> true
    c :: {}
    c -> add' c
    as_str(c) <=> "{{...}}"


    // Dict
    as_str({:}) <=> "{:}"
    as_str({1: 2}) <=> "{1: 2}"
    d :: as_str({1: 2, 3: 4})
    (d == "{1: 2, 3: 4}" || d == "{3: 4, 1: 2}") <=> true

    // Blobs
    ab :: as_str(A { a: 100, b: 5.3 })
    print(ab)

    b : !B = nil
    b = B { self: b }
    b.self = b
    print(b)

    // Functions
    d :: fn x: int, y: int -> int {
        x + y
    }
    print(d)

    // Other
    print' A
    print' pop
    print' :str
    as_str(nil) <=> "nil"
}
