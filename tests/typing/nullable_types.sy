test001 :: fn -> int do
    ret 2
end

test002 :: fn b: bool -> int | void do
    if b do
        ret nil
    else do
        ret 0
    end
end

// TODO(ed): Introduce type type!
test003 :: fn do
    a := test002(false)
    a += 1
    a <=> 1
end

start :: fn do
    test001()
    nil <=> test002(true)
    0 <=> test002(false)
    q: bool | void = true
    q <=> true
    q = nil
    q <=> nil
    test003()
end

